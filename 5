#include<iostream>
using namespace std;

int main()
{
	setlocale(0, "");
	int fing;
	cout << "Введите номер вашего пальца:";
	cin >> fing;
	if (fing == 1) 
		cout << "Большой палец";
	
	 else if (fing == 2) 
		cout << "Указательный палец";
	
	else if (fing == 3) 
		cout << "Средний палец";

	else if (fing == 4) 
		cout << "Безымянный палец";
	
	else if (fing == 5) 
		cout << "Мизинец";
	
	return 0;
}
